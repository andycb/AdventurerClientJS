{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "adventurerclient": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "electronApp",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "electronApp/index.html",
            "main": "electronApp/angularMain.ts",
            "polyfills": "electronApp/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "aot": true,
            "assets": [
              "electronApp/assets"
            ],
            "styles": [
              "electronApp/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "electronApp/environments/environment.ts",
                  "with": "electronApp/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "adventurerclient:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "adventurerclient:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "adventurerclient:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "ElectronApp/test.ts",
            "polyfills": "ElectronApp/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "electronApp/favicon.ico",
              "electronApp/assets"
            ],
            "styles": [
              "electronApp/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "adventurerclient:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "adventurerclient:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "adventurerclient",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}